{
  "env": {
    "development": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
        "@babel/preset-typescript"
      ],
      "plugins": [
        "add-module-exports"
      ]
    },
    "production": {
      "presets": [
        "@babel/preset-env", 
        "minify", 
        "@babel/preset-react",
        "@babel/preset-typescript"
      ], 
      "plugins": [
        "add-module-exports"
      ]
    }
  }
}
