{
  "name": "treble-gsm",
  "version": "1.1.1",
  "description": "Hook based React global state management",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "start": "babel src --watch --out-dir lib --extensions '.ts,.tsx' && tsc --emitDeclarationOnly",
    "clean": "rimraf lib",
    "test": "npm run lint && npm run cover",
    "test:prod": "cross-env BABEL_ENV=production npm run test",
    "test:only": "mocha --require babel-core/register --require babel-polyfill --recursive",
    "test:watch": "npm test -- --watch",
    "test:examples": "node examples/",
    "cover": "nyc --check-coverage npm run test:only",
    "lint": "eslint src test",
    "build": "babel src --out-dir lib --extensions '.ts,.tsx' && tsc --emitDeclarationOnly",
    "prepublish": "npm run build"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hjrdave/treble-gsm.git"
  },
  "keywords": [
    "npm",
    "module",
    "node",
    "javascript"
  ],
  "author": "David Sanders <dsanders@mediafish.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hjrdave/treble-gsm/issues"
  },
  "homepage": "https://github.com/hjrdave/treble-gsm#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.5.0",
    "chai": "^4.1.2",
    "cross-env": "^5.2.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "mocha": "^6.1.3",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
